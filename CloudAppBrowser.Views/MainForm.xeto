<?xml version="1.0" encoding="UTF-8"?>
<Form
  xmlns="http://schema.picoe.ca/eto.forms" 
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Cloud App Browser"
  Width="800"
  Height="640"
  >
  <Splitter Position="200">
    <Splitter.Panel1>
      <GroupBox Text="Environments">
        <TableLayout>
          <TableRow>
            <TableCell>
              <Button Text="Add" Click="AddEnvironment"/>
            </TableCell>
          </TableRow>
          <TableRow>
            <TableCell>
              <TreeView ID="SubsystemTree"/>
            </TableCell>
          </TableRow>
        </TableLayout>
      </GroupBox>
    </Splitter.Panel1>
    <Splitter.Panel2>
      <GroupBox ID="SubsystemPanel" Text="{Binding SubsystemName}">
      </GroupBox>
    </Splitter.Panel2>
  </Splitter>
</Form>